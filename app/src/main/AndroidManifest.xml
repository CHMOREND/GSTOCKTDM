<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ch.orioninformatique.gstocktdm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_tdm"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_tdm_round"
        android:supportsRtl="true"
        android:targetSandboxVersion="1"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="o">
        <activity android:name=".activityRetourClient"></activity>
        <activity
            android:name=".activitycommandeclientListActivity"
            android:label="@string/title_activitycommandeclient_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitycommandeclientDetailActivity"
            android:label="@string/title_activitycommandeclient_detail"
            android:parentActivityName=".activitycommandeclientListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ch.orioninformatique.gstocktdm.activitycommandeclientListActivity" />
        </activity>
        <activity android:name=".activity_recherche_article" />
        <activity android:name=".activity_Parametre" />
        <activity android:name=".activityEntreeMarchandise" />
        <activity android:name=".activity_inventaire" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library android:name="com.symbol.emdk" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.CAMERA"
        tools:ignore="ManifestOrder" />

    <uses-permission android:name="com.symbol.emdk.permission.EMDK" />

</manifest>